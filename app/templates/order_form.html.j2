                                                <div class="svelte-1pzcx2u noUi-target noUi-ltr noUi-horizontal noUi-txt-dir-ltr">
                                                    <div class="noUi-base">
                                                        <div class="noUi-connects">
                                                            <div class="noUi-connect"
                                                                 style="transform: translate(0%, 0px) scale(1, 1)">
                                                            </div>
                                                        </div>
                                                        <div class="noUi-origin"
                                                             style="transform: translate(-100%, 0px);
                                                                    z-index: 5">
                                                            <div class="noUi-handle noUi-handle-lower"
                                                                 data-handle="0"
                                                                 tabindex="0"
                                                                 role="slider"
                                                                 aria-orientation="horizontal"
                                                                 aria-valuemin="-250.0"
                                                                 aria-valuemax="483888.0"
                                                                 aria-valuenow="-250.0"
                                                                 aria-valuetext="-250.00">
                                                                <div class="noUi-touch-area">
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="noUi-origin" style="transform: translate(0%, 0px); z-index: 4">
                                                            <div class="noUi-handle noUi-handle-upper"
                                                                 data-handle="1"
                                                                 tabindex="0"
                                                                 role="slider"
                                                                 aria-orientation="horizontal"
                                                                 aria-valuemin="-250.0"
                                                                 aria-valuemax="483888.0"
                                                                 aria-valuenow="483888.0"
                                                                 aria-valuetext="483888.00">
                                                                <div class="noUi-touch-area">
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <!-- show product -->
                                        <div class="table-container svelte-10rl02j" id="productList">
                                            <table id="table" class="svelte-10rl02j">
                                                <tr class="svelte-10rl02j">
                                                    <th class="svelte-10rl02j">
                                                        <button class="svelte-tn0eqc" style="--right: 0;">
                                                            產品
                                                        </button>
                                                    </th>
                                                    <th class="svelte-10rl02j">
                                                        <tr class="svelte-10rl02j">
                                                            <td class="svelte-10rl02j">
                                                                {% for product in products %}
                                                                    <div style="display: flex; align-items: center;">
                                                                        <div style="margin-right: 1em;">
                                                                            <h2>
                                                                                Product ID: {{ product.id }}
                                                                            </h2>
                                                                            <h2>
                                                                                Product Name: {{ product.name }}
                                                                            </h2>
                                                                            <h2>
                                                                                Product Price: {{ product.price }}
                                                                            </h2>
                                                                        </div>
                                                                        <form method="POST" action="{{ url_for('add_to_cart') }}">
                                                                            <input type="hidden" name="product_id" value="{{ product.id }}">
                                                                            <input type="submit" value="Add to Cart">
                                                                        </form>
                                                                    </div>
                                                                {% endfor %}
                                                            </td>
                                                        </tr>
                                                    </th>
                                                </table>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- cart -->
                            <div>
                                <h3 class="bold">
                                    <i class="glyphicon glyphicon-th-list"></i> Cart Summary
                                </h3>
                                <div class="panel panel-primary">
                                    <div class="panel-body">
                                        Current Cart Value
                                    </div>
                                    <ul class="list-group">
                                        <li class="list-group-item">
                                            <p>
                                                {{ quantity_total }} Items <strong class="pull-right">&dollar;{{ grand_total }}</strong>
                                            </p>
                                            <p>
                                                24hr Shipping <strong class="pull-right">&dollar;100</strong>
                                            </p>
                                        </li>
                                        {% for item, quantity in cart_items %}
                                            <li class="list-group-item">
                                                <p>
                                                    Product Name: {{ item.name }}
                                                </p>
                                                <p>
                                                    Quantity: {{ quantity }}
                                                </p>
                                                <!-- 添加一個 "Remove" 按鈕 -->
                                                <form action="{{ url_for('remove_from_cart', item_id=item.id) }}"
                                                      method="post">
                                                    <input type="submit" value="Remove">
                                                </form>
                                            </li>
                                        {% endfor %}
                                        <li class="list-group-item">
                                            <p>
                                                Total <strong class="pull-right">&dollar;{{ grand_total_plus_shipping }}</strong>
                                            </p>
                                        </li>
                                    </ul>
                                    <div class="panel-footer">
                                        {% if quantity_total>0 %}
                                            <a href="{{ url_for('checkout') }}" class="btn btn-primary btn-sm">
                                                Checkout <i class="glyphicon glyphicon-chevron-right"></i>
                                            </a>
                                            <a href="{{ url_for('clear_cart') }}"
                                               class="btn btn-danger btn-sm pull-right">
                                                Clear Cart <i class="glyphicon glyphicon-trash"></i>
                                            </a>
                                        {% else %}
                                            <a href="#" class="btn btn-primary btn-sm">
                                                Checkout <i class="glyphicon glyphicon-chevron-right"></i>
                                            </a>
                                        {% endif %}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </main>
                </div>
            </div>
        {% endblock %}
